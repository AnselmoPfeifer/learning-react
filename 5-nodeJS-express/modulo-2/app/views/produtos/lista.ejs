<html>
    <head>
        <title>Livro de Node JS</title>
    </head>
    <body>
        <table>
            <tr>
                <td>ID</td>
                <td>Titulo</td>
                <td>Autor</td>
                <td>Data de Produção</td>
            </tr>

            <% for(var i=0;i<lista.length;i++) { %>
            <tr>
                <td><%= lista[i].id %></td>
                <td><%= lista[i].titulo %></td>
                <td><%= lista[i].autor %></td>
                <td><%= lista[i].data_producao %></td>
            </tr>
            <% } %>
        </table>
    </body>
</html>